{
  "expo": {
    "name": "SBKClone",
    "slug": "SBKClone",
    "version": "1.0.0",
    "scheme": "sbkclone",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "tsconfigPaths": true
    },

    "orientation": "portrait",
    "icon": "./src/assets/icon.png",

    "userInterfaceStyle": "light",

    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0f0f0f"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
